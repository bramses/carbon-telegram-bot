  "bugs": {
    "url": "https://github.com/rjmunhoz/carbon-telegram-bot/issues"
  },
  "homepage": "https://github.com/rjmunhoz/carbon-telegram-bot#readme",
  "dependencies": {
    "@nindoo/mongodb-data-layer": "^2.0.5",
    "@sentry/integrations": "^5.6.1",
    "@sentry/node": "^5.6.2",
    "@types/node": "^12.7.2",
    "mongodb": "^3.3.3",
    "ngrok": "^3.4.1",
    "node-fetch": "^2.6.1",
    "puppeteer": "^1.19.0",
